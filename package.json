{
  "name": "diagram-api",
  "version": "0.0.3",
  "main_parcel": "./dist/index.min.js",
  "main": "./build/main/Entry.js",
  "types": "./src/main/Entry.ts",
  "description": "Object Model for Diagrams",
  "scripts": {
    "build": "cd src/examples && NODE_ENV=production ../../node_modules/.bin/ts-node config/fuse.ts",
    "clean": "rm -f -r build/* .cache/*",
    "compile": "bash -c 'cd src; tsc'",
    "deploy": "firebase deploy",
    "example": "src/config/run_example.sh",
    "start": "cd src/examples && NODE_ENV=development ../../node_modules/.bin/ts-node config/fuse.ts",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/diagram-api"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "testRegex": "__tests__/.*\\.(ts|tsx)$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "fuse-box": "^4.0.1-next.7",
    "geom-api": "https://github.com/stephenfrancis/geom-api.git#0.0.5",
    "jest": "^27.4.7",
    "nyc": "^15.1",
    "postcss-modules": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "svg-api": "https://github.com/stephenfrancis/svg-api.git",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
