{
  "name": "diagram-api",
  "version": "0.0.2",
  "main_parcel": "./dist/index.min.js",
  "main": "./build/main/Entry.js",
  "types": "./src/main/Entry.ts",
  "description": "Object Model for Diagrams",
  "scripts": {
    "clean": "rm -f -r build/*",
    "compile": "bash -c 'cd src; tsc'",
    "example": "src/config/run_example.sh",
    "start": "cd src/examples/lgop && NODE_ENV=development ../../../node_modules/.bin/ts-node config/fuse.ts",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/diagram-api"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "__tests__/.*\\.(ts)$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "@types/jest": "^26.0",
    "@types/node": "^15.12",
    "@types/react": "^17.0",
    "@types/react-dom": "^17.0",
    "@types/react-router": "^5.1",
    "@types/react-router-dom": "^5.1",
    "fuse-box": "^4.0.1-next.7",
    "geom-api": "https://github.com/stephenfrancis/geom-api.git",
    "jest": "^27.0",
    "nyc": "^15.1",
    "postcss-modules": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "svg-api": "https://github.com/stephenfrancis/svg-api.git",
    "ts-jest": "^27.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3"
  }
}
